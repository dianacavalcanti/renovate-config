{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>renovatebot/.github"],
  "assignees": ["dcab"],
  "automergeType": "pr",
  "prCreation": "immediate",
  "packageRules": [
    {
      "matchPackageNames": ["renovate"],
      "automerge": true,
      "separateMinorPatch": false,
      "automergeSchedule": [
        "after 1pm and before 3pm"
      ],
      "schedule": [
        "after 1pm and before 3pm"
      ]
    }
  ]
}
